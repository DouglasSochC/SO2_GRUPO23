// memory_tracer.stp


probe syscall.mmap2 {
    printf("Timestamp: %s, PID: %d, Process: %s, Syscall: mmap2, Size: %lu\n",
           ctime(gettimeofday_s()), pid(), execname(), length);
}

probe syscall.munmap {
    printf("Timestamp: %s, PID: %d, Process: %s, Syscall: munmap, Size: %lu\n",
           ctime(gettimeofday_s()), pid(), execname(), length);
}
